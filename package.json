{
  "name": "r-spinners",
  "version": "1.5.0",
  "description": "Custom React Spinners",
  "author": "Punit Soni <punit.soni33@gmail.com> (http://punit-soni.web.app)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PunitSoniME/r-spinners.git"
  },
  "bugs": {
    "url": "https://github.com/PunitSoniME/r-spinners/issues"
  },
  "main": "dist/index.js",
  "module": "dist/r-spinners.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "reactjs",
    "loaders",
    "spinner",
    "spinners",
    "react spinner",
    "react spinners",
    "r spinners",
    "progress"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/r-spinners.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/r-spinners.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "styled-components": "^5.3.5"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
