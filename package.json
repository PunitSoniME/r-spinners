{
  "name": "r-spinners",
  "version": "1.6.0",
  "description": "Custom React Spinners",
  "author": "Punit Soni <punit.soni33@gmail.com> (https://punit-soni.web.app)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PunitSoniME/r-spinners.git"
  },
  "bugs": {
    "url": "https://github.com/PunitSoniME/r-spinners/issues"
  },
  "main": "dist/index.js",
  "module": "dist/r-spinners.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "reactjs",
    "loaders",
    "spinner",
    "spinners",
    "react spinner",
    "react spinners",
    "r spinners",
    "progress"
  ],
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --passWithNoTests",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": true,
    "arrowParens": "avoid",
    "endOfLine": "auto"
  },
  "size-limit": [
    {
      "path": "dist/r-spinners.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/r-spinners.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "dts-cli": "^1.6.3",
    "husky": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "styled-components": "^5.3.5"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
